  "scripts": {
    "start": "npm run build:server && npm run serve",
    "ng": "ng",
    "build": "ng build -op server/dist/public",
    "build:server": "babel server -d server/dist --ignore server/public,server/dist --copy-files",
    "serve": "node server/dist/app.js",
    "watch": "npm run build -- -w",
    "watch:server": "nodemon server/app --exec babel-node",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build -- --aot --prod",
    "validate": "swagger-tools validate server/api/swagger/swagger.json"
  },
