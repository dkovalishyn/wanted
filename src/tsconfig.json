{
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "store/*": ["app/store/*"],
      "shared/*": ["app/shared/*"]
    },
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es7", "dom"],
    "mapRoot": "./",
    "moduleResolution": "node",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": ["../node_modules/@types"]
  }
}
